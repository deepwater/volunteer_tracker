$tr = $("#check-in-<%= @check_in.id %>");
$tr.find('.check-in.date span.value').text("<%= j(@check_in.created_at.strftime('%b %-d'))%>");
$tr.find('.check-out.date span.value').text("<%= j(@check_in.check_out_time.strftime('%b %-d'))%>");
$tr.find('.check-in.time span.value').text("<%= j(@check_in.created_at.strftime('%l:%M%p'))%>");
$tr.find('.check-out.time span.value').text("<%= j(@check_in.check_out_time.strftime('%l:%M%p'))%>");
$tr.find('.hours-worked').text("<%= j(@check_in.hours_worked.to_s)%>");